pipeline {
    agent any
tools{
    maven 'maven'
}
    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main' , url : 'https://github.com/rohansaini-02/PractiseDebOps' 
                echo 'Git Checkout'
            }
        }
        stage('Build'){
            steps{
               sh 'mvn clean package'
            }
        }
        
         stage('Test') {
            steps {
                 sh 'mvn test'
            }
        }
    }
}