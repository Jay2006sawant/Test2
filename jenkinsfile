pipeline {
    agent any
tools{
    maven 'maven'
}
    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main' , url : 'https://github.com/rohansaini-02/PractiseDebOps' 
                echo 'Git Checkout'
            }
        }
        stage('Build'){
            steps{
              echo 'BUILD'
            }
        }
        
         stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('Run JAR'){
            steps{
                script{
                    def output=sh(script: 'java -jar target/simple-java-project-1.0-SNAPSHOT.jar', returnStdout:true).trim()
                    echo "Output from JAR: ${output}"
                }
            }
        }
    }
}