pipeline {
    agent any
tools{
    maven 'maven'
}
    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main' , url : 'https://github.com/rohansaini-02/PractiseDebOps' 
                echo 'Git Checkout'
            }
        }
        stage('Build'){
            steps{
              echo 'BUILD'
            }
        }
        
         stage('Test') {
            steps {
                echo 'Test'
            }
        }
    }
}